-- Scripts:

local function HMGM_fake_script() -- Trigger.LocalScript 
	local script = Instance.new('LocalScript', Trigger)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err =pcall(function()
			for _, v in workspace.Experience:GetChildren() do
				if v:IsA("BasePart") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
					wait()
					v.Touched:Connect(function()v:Destroy()end)
				end
			end
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
		end
	end)
end
coroutine.wrap(HMGM_fake_script)()
local function DXNKC_fake_script() -- Trigger_2.OnActivate 
	local script = Instance.new('LocalScript', Trigger_2)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1.3)
	local x=  TweenInfo.new(0.7)
	local status = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if not status then
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.568627, 0.85098, 0)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,18,0,0)}):Play()
			status = true
			
		else
			ts:Create(script.Parent.Parent.In,info,{BackgroundColor3 = Color3.new(0.239216, 0.239216, 0.239216)}):Play()
			ts:Create(script.Parent.Parent.In.Frame,x,{Position = UDim2.new(0,0,0,0)}):Play()
			status = false
		end
		
		while status do
			pcall(function()
				local sorted = _G.EggToHatch
				local orbstoget = {}
				
				for _, v in workspace.Experience:GetChildren() do
					for __, sort in sorted do
						if string.find(v.Name,sort[1]) then
							table.insert(orbstoget,v)
						end
					end
				end
				
				for ___, orb in orbstoget do
					orb.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				end
			end)
			wait(2)
		end
	end)
end
coroutine.wrap(DXNKC_fake_script)()
local function PWTBRW_fake_script() -- Trigger_3.LocalScript 
	local script = Instance.new('LocalScript', Trigger_3)

	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.3}):Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(1),{BackgroundTransparency = 0.5}):Play()
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		local sucess, err =pcall(function()
			for _, v in workspace.LuckyBlocks:GetChildren() do
				if v:IsA("BasePart") then
					v.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				end
			end
		end)
		
		if not sucess then
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.709804, 0.113725, 0.0705882)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()		
		else
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.12549, 0.615686, 0.141176)}):Play()
			wait(2)
			game:GetService("TweenService"):Create(script.Parent.Parent,TweenInfo.new(2),{BackgroundColor3 = Color3.new(0.219608, 0.219608, 0.219608)}):Play()
		end
	end)
end
coroutine.wrap(PWTBRW_fake_script)()
local function NMDQ_fake_script() -- Trigger_4.LocalScript 
	local script = Instance.new('LocalScript', Trigger_4)

	local status = false
	local isdoing = false
	
	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(3)
	
	local opendrop = UDim2.new(0,176,0,92)
	local openframe = UDim2.new(0,176,0,99)
	
	local closedframe = UDim2.new(0,176,0,0)
	local closeddrop = UDim2.new(0,176,0,0)
	
	script.Parent.MouseButton1Click:Connect(function()
		if isdoing then return end
	
		pcall(function()
			-- триггер
			local frame = script.Parent.Parent.Frame
			local drop = frame.DropDown
			local arrow = frame.Parent.ImageLabel
	
			if not status then
				status = true
				isdoing = true
				-- ебашим
				ts:Create(arrow,TweenInfo.new(1,Enum.EasingStyle.Bounce),{Rotation = 180}):Play()
				local tw = ts:Create(frame,info,{Size = openframe})
				ts:Create(drop,info,{Size = opendrop}):Play()
	
				tw:Play()
				tw.Completed:Wait()
	
				local count = 0
	
				for _, but in drop:GetChildren() do
					if but:IsA("TextLabel") or but:IsA("Frame") or but:IsA("TextButton") or but:IsA("TextBox") then
						count = count + 1
					end
				end
	
				drop.CanvasSize = UDim2.new(0,0,0.35*count,0)
				isdoing = false
			else
				status = false
				isdoing = true
				-- ебашим
				ts:Create(arrow,TweenInfo.new(1,Enum.EasingStyle.Bounce),{Rotation = 0}):Play()
				local tw = ts:Create(frame,info,{Size = closedframe})
				ts:Create(drop,info,{Size = closeddrop}):Play()
	
				tw:Play()
				tw.Completed:Wait()
	
				drop.CanvasSize = UDim2.new(0,0,2,0)
				isdoing = false		
			end 
		end)
	end)
end
coroutine.wrap(NMDQ_fake_script)()
local function XAUSBWD_fake_script() -- DropDown.LocalScript 
	local script = Instance.new('LocalScript', DropDown)

	local sort = {
		{"Basic",false},
		{"Rare",false},
		{"Epic",false},
		{"Legendary",false}
	}
	
	_G.EggToHatch = sort
	
	pcall(function()
		local button = script.Parent.Button
		
		for _, v in {"Basic","Rare","Epic","Legendary"} do
			local cloned = button:Clone()
			cloned.Parent = script.Parent
			cloned.Name = v
			cloned.TextLabel.Font = Enum.Font.Jura
			cloned.Visible = true
			cloned.TextLabel.Text = v
			
			if cloned.Name == _G.EggToHatch then
				cloned.BackgroundColor3 = Color3.new(0.427451, 0.996078, 0.341176)
			end
			
			cloned.TextButton.MouseButton1Click:Connect(function()
				
				for fasfasf, sortv in sort do
					if sortv[1] == cloned.TextLabel.Text then
						if sortv[2] then
							sortv[2] = false
							game:GetService("TweenService"):Create(cloned,TweenInfo.new(1),{BackgroundColor3 = Color3.new(0.996078, 0.996078, 0.996078)}):Play()
						else
							sortv[2] = true
							game:GetService("TweenService"):Create(cloned,TweenInfo.new(1),{BackgroundColor3 = Color3.new(0.427451, 0.996078, 0.341176)}):Play()
						end
					end
				end
				
				_G.EggToHatch=sort
				
			end)
		end
	end)
end
coroutine.wrap(XAUSBWD_fake_script)()
local function MUEJX_fake_script() -- TextLabel_6.Script 
	local script = Instance.new('LocalScript', TextLabel_6)

	local ts = game:GetService("TweenService")
	local info = TweenInfo.new(1)
	script.Parent.Font = Enum.Font.Michroma
	script.Parent.MouseEnter:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{TextColor3 = Color3.new(math.random(0.1,1),math.random(0.1,1),math.random(0.1,1))}):Play()
		end)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		pcall(function()
			ts:Create(script.Parent,info,{TextColor3 = Color3.new(1,1,1)}):Play()
		end)
	end)
end
coroutine.wrap(MUEJX_fake_script)()
local function TEEXLVT_fake_script() -- Toggle_2.OnActivate 
	local script = Instance.new('LocalScript', Toggle_2)

	local toggle = script.Parent
	local ts= game:GetService("TweenService")
	local info = TweenInfo.new(1.25,Enum.EasingStyle.Cubic)
	
	local status = false
	local isalreadydoing = false	
	
	script.Parent.MouseButton1Click:Connect(function()
		if isalreadydoing then return end
		local sucess, err = pcall(function()
			local closingtab = script.Parent.Parent.Parent
			
			if not status then
			isalreadydoing = true
			status = true
			ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =180}):Play()
			local tween = ts:Create(closingtab,info,{Size = UDim2.new(0,210,0,35)})
			local x = ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 1})
			closingtab.Container.ContainerList.Visible = false
			tween:Play()
			x:Play()
			x.Completed:Wait()
			tween.Completed:Wait()
			isalreadydoing = false
			else
				status = false
				isalreadydoing = true
				local tween = ts:Create(closingtab,info,{Size = UDim2.new(0,210,0,276)})
				ts:Create(script.Parent,TweenInfo.new(1,Enum.EasingStyle.Cubic),{Rotation =0}):Play()
				tween:Play()
				tween.Completed:Wait()
				ts:Create(closingtab.Container,TweenInfo.new(0.5),{BackgroundTransparency = 0.5}):Play()
				isalreadydoing = false
				closingtab.Container.ContainerList.Visible = true
			end
		end)
		
		if not sucess then print(err) end
	end)
end
coroutine.wrap(TEEXLVT_fake_script)()
